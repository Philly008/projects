	float: left;
	background-image: url(../images/button_bg3a.jpg);
	text-align: center;
	height: 22px;
	width: 34px;
	line-height: 22px;
	font-weight: bold;
}
.buttonstyle_05 a {
	display: block;
	height: 22px;
}
.buttonstyle_05 a:hover {
	background-image: url(../images/button_bg3b.jpg);
}
.buttonstyle_06     {
	background-image: url(../images/button_bg3a.jpg);
