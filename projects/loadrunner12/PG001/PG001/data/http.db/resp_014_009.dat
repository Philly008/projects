<script type="text/javascript">
	function nextPageNew(){
		
		var lastPage = parseInt($("#lastPageNoNew").val());
		var currentPage = parseInt($("#pageNoNew").val());
		if(currentPage<lastPage){
			$("#pageNoNew").val(currentPage+1);
			pageQueryNew();
		}
	}

	function previousPageNew(){
		var currentPage = parseInt($("#pageNoNew").val());
		if(currentPage>1){
			$("#pageNoNew").val(currentPage-1);
			pageQueryNew();
		}
	}

	function firstPageNew(){
		var currentPage = parseInt($("#pageNoNew").val());
		if(currentPage!=1){
			$("#pageNoNew").val(1);
			pageQueryNew();	
		}
	}

	function lastPageNew(){
		var currentPage = parseInt($("#pageNoNew").val());
		var lastPage=parseInt($("#lastPageNoNew").val());
		if(currentPage!=lastPage){
			$("#pageNoNew").val(lastPage);
			pageQueryNew();	
		}
	}

	function selectPageNew(){
		var currentPage = parseInt($("#pageNoNew").val());
		var selectPageNo = $("#pageSelectNew").val();
		if(currentPage!=selectPageNo){
			$("#pageNoNew").val(selectPageNo);
			pageQueryNew();	
		}		
	}
	
</script>
<input type="hidden" id="pageNoNew" name="page.pageNo" value="1">
<input type="hidden" id="lastPageNoNew" value="0">
<input type="hidden" id="totalPageNoNew" value="0">

<ul class="page">
  <li><input type="button" value="首页" class="btnPage" onclick="firstPageNew();" /></li>
  <li><input type="button" value="上一页" class="btnPageLong" onclick="previousPageNew();" /></li>
  <li>
  	<select id="pageSelectNew" class="selectPage" onchange="selectPageNew();">
  		 
	</select>
  </li>
  <li><input type="button" value="下一页" class="btnPageLong" onclick="nextPageNew();"/></li>
  <li id="lastpageNew"><input value="尾页" type="button" class="btnPage"   onclick="lastPageNew();" /></li>
</ul>

