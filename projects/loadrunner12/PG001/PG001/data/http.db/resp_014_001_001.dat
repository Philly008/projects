    -moz-box-shadow: 2px 0px 2px 0px rgba(0, 0, 0, 0.2);
    box-shadow: 2px 0px 2px 0px rgba(0, 0, 0, 0.2); 
}
     
ul.menu-nav > li > a:hover, ul.menu-nav > li.current  > a {
    color: #036564;
    text-shadow: 0px 1px 0px #EBEBEB;
}
*/
ul.menu-nav > li > a:hover {background: url(../images/top_menu_over.png) no-repeat top center; display:block; width:90px; height:44px; }
ul.menu-nav > li.current  > a {background: url(../images/top_menu_on.png) no-repeat top center; display:block; width:100px; height:44px; color:#fff; }
ul.menu-nav li a {
    color: #00437e;
	font-weight:bold;
	line-height:32px;
    padding: 5px;
    text-decoration: none;
	width:90px;
	text-align:center;
}   

ul.menu-nav li a:hover {
    text-decoration: none;
}
                
ul.menu-nav ul.child {
    border-top: none;
    border-right: 1px solid #07273b;
    border-bottom: 1px solid #07273b;
    margin-top: -5px;
}

ul.menu-nav ul.child li a, ul.menu-nav ul.child ul.grandchild li a {
    font-family: Verdana, Arial,"宋体",Helvetica, sans-serif;
    font-size: 12px;
    color: #FFFFFF;
    
    letter-spacing: normal;
    white-space: nowrap;
    text-transform: none;
    background: #216088 url(../images/submenu_shadow.png) no-repeat bottom center;
	opacity:0.9;
}
